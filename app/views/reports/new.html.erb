<% content_for :head do %>
  <script type="text/javascript">
    // Initialize the js for this particular page when the DOM is ready.
    $(document).ready(function() {
      app.reports.initialize({});
    });
  </script>
<% end %>

<% provide(:page_id, "reports-new-page") %>
<% provide(:page_class, "scroll-aware") %>

<%= render 'reports/common/input_tool' %>

<%= render 'reports/sprint_release/sprint_release' %>

<div id="import-button" class="js-import-button import-button">
  <i></i><span>Import</span>
</div>

<div id="save-button" class="js-save-button save-button">
  <i></i><span>Save</span>
</div>

<div id="download-button" class="js-download-button download-button">
  <i></i><span>Download</span>
</div>